<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="rectangle">
            <gradient
                android:angle="90"
                android:centerColor="#FFFFFF"
                android:endColor="#FDFDFD"
                android:startColor="#EAEAEA"/>
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="2dp"
                android:color="#EAEAEA" />
        </shape>
    </item>
</layer-list>